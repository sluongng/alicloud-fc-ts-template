{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "./src/*": [
                "../dist/*"
            ]
        },
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist/",
        "sourceMap": true,
        "target": "es2016"
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}