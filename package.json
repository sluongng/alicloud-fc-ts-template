{
  "name": "vnwork",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "node ./dist/index.test.js",
    "build": "tsc",
    "create": ". env-setup.sh && fcli function create --service-name ${SERVICE_NAME} --function-name ${FUNCTION_NAME} --handler ${HANDLER} --runtime nodejs8 --code-dir .",
    "update": ". env-setup.sh && fcli function update --service-name ${SERVICE_NAME} --function-name ${FUNCTION_NAME} --handler ${HANDLER} --runtime nodejs8 --code-dir .",
    "invoke": ". env-setup.sh && fcli function invoke --service-name ${SERVICE_NAME} --function-name ${FUNCTION_NAME}",
    "logs": ". env-setup.sh && fcli function logs --service-name ${SERVICE_NAME} --function-name ${FUNCTION_NAME} --start $(date -v -5M -u '+%Y-%m-%dT%H:%M:%SZ')",
    "trigger-create": ". env-setup.sh && fcli trigger create --service-name ${SERVICE_NAME} --function-name ${FUNCTION_NAME} --trigger-name ${TRIGGER_NAME} --type ${TRIGGER_TYPE} --config ${TRIGGER_CONFIG}",
    "trigger-update": ". env-setup.sh && fcli trigger update --service-name ${SERVICE_NAME} --function-name ${FUNCTION_NAME} --trigger-name ${TRIGGER_NAME} --trigger-config ${TRIGGER_CONFIG}",
    "live-test": "npm run build && npm run update && npm run invoke && npm run logs",
    "all": "npm run build && npm"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "raw-body": "^2.3.3"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  }
}
